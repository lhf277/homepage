{
    "redirects": [
        {
            "source": "$host",
            "destination": "$wwwhost",
            "statusCode": 301
        }
    ],
    "headers": [
      {
        "source": "/*",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "Cache-Control",
            "value": "max-age=7200"
          }
        ]
      }
    ],
    "caches": [
      {
        "source": "/*",
        "cacheTtl": 86400
      }
    ]

}
